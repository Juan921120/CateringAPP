page {
	width: 100%;
	height: 100%;
}
.recordBg {
	width: 100%;
	height: 258px;
}
.record-item {
	width: 690rpx;
	height: 260rpx;
	background: rgba(255, 255, 255, 1);
	box-shadow: 0px 5rpx 51rpx 0px rgba(172, 117, 138, 1);
	border-radius: 20rpx;
	padding: 50rpx 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	margin: 0 auto;
}
.R1 {
	font-size: 60rpx;
	font-family: PingFang SC;
	font-weight: bold;
	color: rgba(0, 0, 0, 1);
	line-height: 36rpx;
}
.recordNumber,
.recordAdress {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	        justify-content: flex-start;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.recordNumber {
	margin-top: 33rpx;
}
.recordAdress {
	margin-top: 23rpx;
}
.recordNumber image {
	width: 30rpx;
	height: 30rpx;
	padding-right: 12rpx;
}
.recordAdress image {
	width: 32rpx;
	height: 28.8rpx;
	padding-right: 12rpx;
}
.recordDetail {
	width: 50%;
	margin-left: 49rpx;
}
.N1 {
	font-size: 28rpx;
	font-family: PingFang SC;
	font-weight: 400;
	color: rgba(34, 34, 34, 1);
	line-height: 36rpx;
}
.N2 {
	font-size: 26rpx;
	font-family: PingFang SC;
	font-weight: 400;
	color: rgba(34, 34, 34, 1);
	line-height: 36rpx;
}
.mother {
	width: 144rpx;
	height: 144rpx;
	border-radius: 50%;
	margin-right: 54rpx;
}
.mother image {
	width: 144rpx;
	height: 144rpx;
}
.single {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin: 50rpx 31rpx 21rpx 34rpx;
}
.singleTitle {
	font-size: 36rpx;
	font-family: PingFang SC;
	font-weight: 500;
	color: rgba(17, 17, 17, 1);
	line-height: 80rpx;
}
.total {
	font-size: 24rpx;
	font-family: PingFang SC;
	font-weight: 500;
	color: rgba(17, 17, 17, 1);
	line-height: 80rpx;
}
.singleDetail {
	width: 690rpx;
	height: 180rpx;
	background: rgba(255, 255, 255, 1);
	box-shadow: 0px 9rpx 35rpx 5rpx rgba(172, 117, 138, 0.43);
	border-radius: 8rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	margin: 0 auto 32rpx;
}
.left {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.singleDate,
.singleCategory {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.singleDate image,
.singleCategory image {
	width: 48rpx;
	height: 48rpx;
	margin-right: 20rpx;
	margin-left: 26rpx;
}
.line {
	width: 460rpx;
	height: 2rpx;
	background: -webkit-linear-gradient(left,  rgba(248, 230, 233, 1), rgba(255, 255, 255, 1), rgba(255, 255, 255, 0));
	background: linear-gradient(90deg,  rgba(248, 230, 233, 1), rgba(255, 255, 255, 1), rgba(255, 255, 255, 0));
	margin-left: 94rpx;
}
.dates,
.category {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.right image {
	width: 15.9rpx;
	height: 27.8rpx;
}
.detail {
	padding-right: 14rpx;
	font-size:24rpx;
	font-family:PingFang SC;
	font-weight:400;
	color:rgba(204,204,204,1);
	line-height:80rpx;
}
.right {
	margin-right: 19rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.D1{
	font-size:20rpx;
	font-family:PingFang SC;
	font-weight:400;
	color:rgba(102,102,102,1);
	padding-top: 14rpx;
}
.S1{
	font-size:25rpx;
	font-family:PingFang SC;
	font-weight:500;
	color:rgba(17,17,17,1);
	padding-bottom: 17rpx;
}

