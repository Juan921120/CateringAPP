.bg
	{
	width:750rpx;
	height:1942rpx;
	position:relative;
	animation:mymove 5s infinite;
	-webkit-animation:mymove 5s 1; /*Safari and Chrome*/
}
@-webkit-keyframes mymove
	{
from {bottom:0px;}
to {bottom:600rpx;}
}
@keyframes mymove
	{
from {bottom:0px;}
to {bottom:600rpx;}
}

